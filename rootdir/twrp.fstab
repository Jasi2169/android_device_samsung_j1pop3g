/boot		emmc		/dev/block/mmcblk0p20
/recovery	emmc		/dev/block/mmcblk0p21				flags=backup=1
/data		ext4		/dev/block/mmcblk0p26		length=-16384
/system		ext4		/dev/block/mmcblk0p24
/cache		ext4		/dev/block/mmcblk0p23				flags=backup=1;display=cache
/efs		emmc		/dev/block/mmcblk0p17				flags=backup=1;display=EFS
/modem		emmc		/dev/block/mmcblk0p8				flags=backup=1;display="Modem"
/modem2		emmc		/dev/block/mmcblk0p8				flags=backup=1;display="Modem2"
/external_sd	vfat		/dev/block/mmcblk1p1	/dev/block/mmcblk1	flags=display="Micro SD";storage;wipeingui;removable
